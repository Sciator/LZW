{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/index.ts",
      "outFiles": [
        "${workspaceFolder}/bin/**/*.js"
      ],
      "args": [
        "./data/test.txt"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Decompress",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/index.ts",
      "outFiles": [
        "${workspaceFolder}/bin/**/*.js"
      ],
      "args": [
        "./data/test.txt.lzw",
        "./data/test.out.txt"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "PDF Compress",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/index.ts",
      "outFiles": [
        "${workspaceFolder}/bin/**/*.js"
      ],
      "args": [
        "./data/test.pdf"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}